{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [2, 4,{ "SwitchCase": 1 }],
        "comma-dangle": ["error", "never"],
        "array-bracket-spacing":["error","never"],
        "no-console": "off",
        "no-else-return": "off"
    }
}